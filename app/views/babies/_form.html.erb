<!-- Your form goes here! -->
<%= form_for(@baby) do |f| %>
	<div class="field">
		<%= f.label :first_name %> <br>
		<%= f.text_field :first_name %>
	</div>
	<div class="field">
		<%= f.label :last_name %> <br>
		<%= f.text_field :last_name %>
	</div>
	<div class="field">
		<%= f.label :weight %> <br>
		<%= f.number_field :weight %>
	</div>
	<div class="field">
		<%= f.label :birth_date %> <br>
		<%= f.text_field :birth_date %>
	</div>
	<div class="actions">
    <%= f.submit %>
  </div>
<% end %>
